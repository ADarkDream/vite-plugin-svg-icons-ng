{
  "name": "vite-plugin-svg-icons-monorepo",
  "private": true,
  "version": "2.0.1",
  "license": "MIT",
  "author": "Vben",
  "scripts": {
    "stub": "pnpm --filter ./packages run prepack -- --stub",
    "postinstall": "pnpm run stub",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"packages/**/*.{ts,tsx}\" --fix",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "test": "vitest"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "svg",
    "sprite",
    "svgo",
    "vben"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vbenjs/vite-plugin-svg-icons"
  },
  "bugs": {
    "url": "https://github.com/vbenjs/vite-plugin-svg-icons/issues"
  },
  "homepage": "https://github.com/vbenjs/vite-plugin-svg-icons/tree/main/#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.21.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.5",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "unbuild": "^3.5.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.6"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "packages/*/{src,types}/**/*.ts": [
      "eslint --ext .ts"
    ],
    "packages/**/*.d.ts": [
      "eslint --ext .ts"
    ]
  }
}
